    "/crawlers": {
      "get": {
        "tags": [
          "Crawlers"
        ],
        "summary": "Retrieves all crawlers associated with the specified application key.",
        "parameters": [
          {
            "name": "appKey",
            "in": "query",
            "description": "The unique identifier for the application whose crawlers are to be retrieved. Must be a valid application\r\n    key.",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of crawlers succesfully retrieved  for thespecified application key successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Crawler"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Crawler"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Crawler"
                  }
                }
              }
            }
          },
          "401": {
            "description": "A valid app key is required",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Crawlers"
        ],
        "summary": "Creates a new labyrinth crawler instance from a maximum of 3 associated with the specified application key.",
        "description": "All crawlers must be deleted to leave a labyrinth instance associated with an application key.",
        "parameters": [
          {
            "name": "appKey",
            "in": "query",
            "description": "The unique application key used to authorize the request and associate the crawler instance. Must be a\r\n    valid, registered app key.",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "description": "Optional settings to configure the crawler and labyrinth. Ignored if the app key already has a crawler.",
          "content": {
            "application/json": {
              "schema": {
                "oneOf": [
                  {
                    "type": "null"
                  },
                  {
                    "$ref": "#/components/schemas/Settings"
                  }
                ]
              }
            },
            "text/json": {
              "schema": {
                "oneOf": [
                  {
                    "type": "null"
                  },
                  {
                    "$ref": "#/components/schemas/Settings"
                  }
                ]
              }
            },
            "application/*+json": {
              "schema": {
                "oneOf": [
                  {
                    "type": "null"
                  },
                  {
                    "$ref": "#/components/schemas/Settings"
                  }
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Crawler successfully created",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/Crawler"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Crawler"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/Crawler"
                }
              }
            }
          },
          "400": {
            "description": "Bad parameters in settings see Settings fields documentation",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "401": {
            "description": "A valid app key is required",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "403": {
            "description": "This app key reached its 3 instances of simultaneous crawlers",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
